{
  "name":"AppSpider Validate",
  "version": "0.0.1",
  "manifest_version": 2,
  "background": {
    "scripts":["validate.js"],
    "persistent": false
  },
  "browser_action": {
    "default_icon":"icon.png",
    "default_popup":"popup.html"
  },
  "content_script":[{
    "matches": ["<all_urls>"],
    "js":["open-dialog.js"]
  }]
}